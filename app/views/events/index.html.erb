<div class="container">
  <h1 class="text-center pt-4 pb-4">List of events</h1>
  <section class="upcoming-events">
    <h1 class="text-center pt-4 pb-4">Upcoming events</h1>
    <%= render 'shared/events_list', object: @upcoming_events %>
  </section>
  <section class="past-events">
    <h1 class="text-center pt-4 pb-4">Previous events</h1>
    <%= render 'shared/events_list', object: @past_events %>
  </section>
</div>